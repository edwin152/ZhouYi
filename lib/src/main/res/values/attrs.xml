<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Rect">
        <attr name="rect_radius" format="dimension" />
        <attr name="rect_leftTopRadius" format="dimension" />
        <attr name="rect_rightTopRadius" format="dimension" />
        <attr name="rect_rightBottomRadius" format="dimension" />
        <attr name="rect_leftBottomRadius" format="dimension" />
        <attr name="rect_strokeWidth" format="dimension" />
